{
    "name": "janpiet/php-version-transpiler",
    "description": "A shim for the new namespaces of the php code coverage lib by sebstian bergmann",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "phpunit/php-code-coverage": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload-dev": {
        "psr-4": {
            "JanPiet\\Tests\\CodeCoverageShim\\": "tests"
        }
    },
    "autoload": {
        "files": [
            "src/autoload.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Pietrzyk",
            "email": "mail@jpietrzyk.de"
        }
    ]
}
